{
  "name": "express-the-right-way",
  "version": "1.0.0",
  "author": "John Viscel Sangkal <jv@codeninja.co>",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "build": "rimraf lib dist && babel src -d lib --ignore .test.js",
    "lint": "eslint src --ext .js,.jsx",
    "test": "yarn lint && flow && yarn jest",
    "jest": "jest --coverage --verbose --passWithNoTests",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-inline-json-import": "^0.2.1",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.66.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "nodemon": "^1.17.1",
    "pm2": "^2.10.1",
    "prettier": "1.11.1",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
